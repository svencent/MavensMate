{
  "name": "mavensmate",
  "version": "1.0.0-beta.32",
  "description": "Core APIs that drive MavensMate IDEs for Salesforce1/Force.com",
  "private": false,
  "bin": {
    "mavensmate": "./bin/mavensmate"
  },
  "main": "./app/lib/mavensmate",
  "scripts": {
    "start": "bin/server --port=56248",
    "test": "make test",
    "coverage": "make coverage",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "blanket": {
      "pattern": "lib/",
      "data-cover-never": "node_modules"
    }
  },
  "author": {
    "name": "Joe Ferraro",
    "email": "ferraro.joseph@gmail.com",
    "url": "https://github.com/joeferraro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeferraro/mavensmate.git"
  },
  "keywords": [
    "tools",
    "salesforce",
    "salesforce1",
    "force.com",
    "visualforce",
    "apex",
    "lightning",
    "mavensmate",
    "ide"
  ],
  "license": "GPLv3",
  "bugs": "https://github.com/joeferraro/mavensmate/issues",
  "dependencies": {
    "archiver": "0.12.0",
    "autocmdr": "0.0.7",
    "bluebird": "~2.4.2",
    "body-parser": "1.9.2",
    "commander": "joeferraro/commander.js",
    "events": "1.0.2",
    "express": "4.10.2",
    "findit": "2.0.0",
    "fs-extra": "0.22.1",
    "fs-extra-promise": "^0.1.2",
    "fstream": "1.0.2",
    "graceful-fs": "^4.1.6",
    "inherits": "2.0.1",
    "jsforce": "1.7.0",
    "lodash": "3.10.1",
    "merge": "1.2.0",
    "minimist": "1.1.0",
    "moment": "2.8.3",
    "nconf": "0.6.9",
    "open": "0.0.3",
    "request": "2.74.0",
    "sax": "0.6.1",
    "socket.io": "^1.3.7",
    "strip-json-comments": "1.0.2",
    "swig": "joeferraro/swig",
    "temp": "0.8.1",
    "uuid": "^3.0.0",
    "which": "1.0.5",
    "winston": "0.9.0",
    "xml2js": "0.4.4",
    "xmldoc": "0.1.2",
    "yauzl": "^2.6.0"
  },
  "optionalDependencies": {
    "keytar": "3.0.2"
  },
  "devDependencies": {
    "blanket": "1.1.5",
    "chai": "1.10.0",
    "chai-fs": "0.1.0",
    "coveralls": "^2.11.12",
    "grunt-nodemon": "latest",
    "istanbul": "^0.4.5",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "1.17.2",
    "sinon-as-promised": "latest",
    "supertest": "^2.0.0"
  }
}
